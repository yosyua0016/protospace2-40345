<main class="main">
  <div class="inner">
    
    <% if user_signed_in? %>
      <div class="greeting">
        こんにちは、<%= link_to current_user.name, root_path, class: :greeting__link %>
      </div>
    <% end %>
      
    <<div class="card__wrapper">
      <% @prototypes.each do |prototype| %>
        <div class="card">
          <h2><%= prototype.title %></h2>
          <p><%= prototype.catch_copy %></p>
          <!-- 他のプロトタイプの情報を表示する -->
        </div>
      <% end %>
  </div>
</main>
